@model SantaProject.Models.Toys

@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="col-xs-8">
        <h2 style="text-align:center;">Toys</h2>
    </div>
</div>
@foreach (var toys in Model.EntityList)
{
    <hr />
    <div class="row">
        <div class="col-xs-8"><h4>@Html.Display(toys.Name.ToString())</h4></div>
        <div class="col-xs-2">@Html.Display("Ciao")</div>
    </div>

}
